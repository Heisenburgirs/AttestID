{
  "name": "optimism-starter",
  "version": "1.0.8",
  "private": true,
  "scripts": {
    "anvil": "source .env && anvil --fork-url $ANVIL_FORK_URL --chain-id $ANVIL_CHAIN_ID",
    "deploy": "forge create contracts/src/AttestId.sol:AttestId --rpc-url $FORGE_RPC_URL --constructor-args $WORLD_ID $WORLD_APP_ID $WORLD_ACTION_ID --private-key $FORGE_PRIVATE_KEY --etherscan-api-key $ETHERSCAN_API_KEY --verify",
    "deploy:anvil": "source .env && forge create contracts/src/AttestationStation.sol:AttestationStation --rpc-url http://127.0.0.1:8545 --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 --legacy",
    "dev": "npm run && vite",
    "dev:foundry": "npm run dev & wagmi generate --watch & npm run anvil",
    "build": "npm run wagmi && vite build",
    "build:production": "vite build",
    "preview": "vite preview",
    "lint": "prettier --write \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "wagmi": "wagmi generate"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@eth-optimism/atst": "^0.0.0-20230306200511",
    "@eth-optimism/contracts-bedrock": "^0.12.1",
    "@eth-optimism/contracts-periphery": "^1.0.7",
    "@eth-optimism/sdk": "0.0.0-20230301065104",
    "@ethereum-attestation-service/eas-sdk": "^1.0.0-beta.0",
    "@openzeppelin/contracts": "4.7.3",
    "@openzeppelin/contracts-upgradeable": "4.7.3",
    "@rainbow-me/rainbowkit": "^1.0.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@wagmi/cli": "^1.3.0",
    "@worldcoin/idkit": "^0.5.1",
    "autoprefixer": "^10.4.14",
    "buffer": "^6.0.3",
    "ethers": "^5.7.2",
    "ethers-utils": "^2.1.11",
    "graphql": "^16.7.1",
    "html-to-image": "^1.11.11",
    "ipfs-http-client": "^60.0.1",
    "jsx-to-png": "^1.0.0-beta.1",
    "postcss": "^8.4.27",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "process": "^0.11.10",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "util": "^0.12.4",
    "viem": "^1.4.2",
    "wagmi": "^1.3.9",
    "web3": "^4.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^3.1.0",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
